{
  "version": 2,
  "env": {
    "DB_URI": "@db_uri"
  },
  "builds": [
    {
      "src": "src/main.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/upload/assets/(.*)",
      "dest": "src/upload/assets/$1",
      "methods": ["GET"]
    },
    {
      "src": "/(.*)",
      "dest": "src/main.ts",
      "methods": ["GET", "POST", "PUT", "DELETE"]
    }
  ]
}
